{
  "proxy": {
    "enabled": true,
    "rotation_interval": 300,
    "max_requests_per_proxy": 10,
    "proxy_sources": [
      "custom_proxy_list"
    ],
    "residential_preferred": true,
    "geographic_distribution": true
  },
  "search": {
    "base_url": "https://locator.ice.gov",
    "timeout": 30,
    "max_retries": 3,
    "requests_per_minute": 10,
    "fuzzy_threshold": 0.7,
    "human_delays": true
  },
  "cache": {
    "enabled": true,
    "ttl": 3600,
    "backend": "diskcache"
  },
  "security": {
    "log_sensitive_data": false,
    "anonymize_logs": true,
    "encrypt_cache": false
  }
}